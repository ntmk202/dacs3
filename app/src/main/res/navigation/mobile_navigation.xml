<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.ntmk.myapp.ui.home.HomeFragment"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_garden"
        android:name="com.ntmk.myapp.ui.garden.GardenFragment"
        tools:layout="@layout/fragment_garden" />

    <fragment
        android:id="@+id/navigation_live"
        android:name="com.ntmk.myapp.ui.live.LiveFragment"
        tools:layout="@layout/fragment_live" />

    <fragment
        android:id="@+id/navigation_notification"
        android:name="com.ntmk.myapp.ui.notifications.NotificationsFragment"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.ntmk.myapp.ui.profile.ProfileFragment"
        tools:layout="@layout/fragment_profile" />
</navigation>